{
    "manifest_version": 3,
    "name": "Chrome Version Checker",
    "version": "1.0",
    "description": "Warns if Google Chrome is out of date.",
    "permissions": ["alarms", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/normal-icon16.png",
      "48": "icons/normal-icon48.png",
      "128": "icons/normal-icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/normal-icon16.png"
    }
  }
  